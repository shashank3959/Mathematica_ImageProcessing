Cell["\<\
Extracting a column is little more difficult. The simplest way is to \
transpose the matrix and then take its desired row (which obviously \
corresponds to the original column). For example the second column of matrix \
m is extracted as follows.\
\>", "Text",ExpressionUUID->"b4f7eebb-8bed-4db0-ba8b-093a537fd834"],

Cell[CellGroupData[{

Cell["c2 = Transpose[m][[2]]", "Input",ExpressionUUID->"49249952-2c8a-4fa1-aa4c-dbac0bee5163"],

Cell[BoxData[
 FormBox[
  RowBox[{"{", 
   RowBox[{"2", ",", "6", ",", "10"}], "}"}], TraditionalForm]], "Output",Expr\
essionUUID->"864ef814-f4ff-4f44-9f28-a79357ee8e4d"]
}, Open  ]],

Cell["\<\
Matrices can also be defined in terms of symbolic expressions. For example \
here is a 2x2 matrix called m.\
\>", "Text",ExpressionUUID->"fdd63c22-869f-41cd-aabf-a9a4d266c7ed"],

Cell[CellGroupData[{

Cell["m = {{y, 2*x}, {(3*y)/x^3, 4*y}}; MatrixForm[m]", "Input",ExpressionUUID->"a525ce13-fa27-4e83-8675-5424b28b55e6"],

Cell[BoxData[
 FormBox[
  TagBox[
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {"y", 
       RowBox[{"2", " ", "x"}]},
      {
       FractionBox[
        RowBox[{"3", " ", "y"}], 
        SuperscriptBox["x", "3"]], 
       RowBox[{"4", " ", "y"}]}
     }], "\[NoBreak]", ")"}],
   Function[BoxForm`e$, 
    MatrixForm[BoxForm`e$]]], TraditionalForm]], "Output",ExpressionUUID->\
"48da2591-85cb-42c4-b27a-05d2bde9f3f7"]
}, Open  ]],

Cell["Determinant of m", "Text",ExpressionUUID->"e0b6e4cb-a943-4db6-8acf-cc39371c56f0"],

Cell[CellGroupData[{

Cell["Det[m]", "Input",ExpressionUUID->"fa160930-e459-4793-b6df-ee51140fad84"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"4", " ", 
    SuperscriptBox["y", "2"]}], "-", 
   FractionBox[
    RowBox[{"6", " ", "y"}], 
    SuperscriptBox["x", "2"]]}], TraditionalForm]], "Output",ExpressionUUID->\
"954d3791-d75d-45bf-8243-b466fbb33d03"]
}, Open  ]],

Cell["Inverse of m", "Text",ExpressionUUID->"efdb08c5-a7fd-4d5f-9966-4eae10401759"],

Cell[CellGroupData[{

Cell["mi = Inverse[m]; MatrixForm[mi]", "Input",ExpressionUUID->"a81a0803-5cce-4f5d-98e6-11d58512335a"],

Cell[BoxData[
 FormBox[
  TagBox[
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {
       FractionBox[
        RowBox[{"4", " ", "y"}], 
        RowBox[{
         RowBox[{"4", " ", 
          SuperscriptBox["y", "2"]}], "-", 
         FractionBox[
          RowBox[{"6", " ", "y"}], 
          SuperscriptBox["x", "2"]]}]], 
       RowBox[{"-", 
        FractionBox[
         RowBox[{"2", " ", "x"}], 
         RowBox[{
          RowBox[{"4", " ", 
           SuperscriptBox["y", "2"]}], "-", 
          FractionBox[
           RowBox[{"6", " ", "y"}], 
           SuperscriptBox["x", "2"]]}]]}]},
      {
       RowBox[{"-", 
        FractionBox[
         RowBox[{"3", " ", "y"}], 
         RowBox[{
          SuperscriptBox["x", "3"], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"4", " ", 
             SuperscriptBox["y", "2"]}], "-", 
            FractionBox[
             RowBox[{"6", " ", "y"}], 
             SuperscriptBox["x", "2"]]}], ")"}]}]]}], 
       FractionBox["y", 
        RowBox[{
         RowBox[{"4", " ", 
          SuperscriptBox["y", "2"]}], "-", 
         FractionBox[
          RowBox[{"6", " ", "y"}], 
          SuperscriptBox["x", "2"]]}]]}
     }], "\[NoBreak]", ")"}],
   Function[BoxForm`e$, 
    MatrixForm[BoxForm`e$]]], TraditionalForm]], "Output",ExpressionUUID->\
"2a30f61c-226e-449b-9601-150c479e4c13"]
}, Open  ]],

Cell["Check the inverse", "Text",ExpressionUUID->"a18f8f85-eb5c-467d-9acf-3ea0d0ec4dc8"],

Cell[CellGroupData[{

Cell["Simplify[mi.m]", "Input",ExpressionUUID->"0017cd41-168c-4494-bf50-a0c25a9ebaf1"],

Cell[BoxData[
 FormBox[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {"1", "0"},
     {"0", "1"}
    },
    GridBoxAlignment->{
     "Columns" -> {{Decimal}}, "ColumnsIndexed" -> {}, "Rows" -> {{Baseline}},
       "RowsIndexed" -> {}}], "\[NoBreak]", ")"}], TraditionalForm]], "Output",\
ExpressionUUID->"3a95523d-8511-4683-a113-81c066d47246"]
}, Open  ]],

