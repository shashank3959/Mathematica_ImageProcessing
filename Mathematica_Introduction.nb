      RowBox[{"7", ",", "9", ",", "2"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", "2", ",", "13"}], "}"}]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"R", "=", 
